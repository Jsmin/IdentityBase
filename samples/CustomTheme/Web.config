<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.web>
    <customErrors defaultRedirect="errorpage.htm" mode="Off">
    </customErrors>
  </system.web>
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified"/>
    </handlers>
    <aspNetCore processPath="%LAUNCHER_PATH%" arguments="%LAUNCHER_ARGS%" stdoutLogEnabled="false" stdoutLogFile=".\TempData" forwardWindowsAuthToken="false"/>
  </system.webServer>
</configuration>